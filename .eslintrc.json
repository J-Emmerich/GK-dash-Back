{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb", "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react", "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-useless-catch": "off",
        "no-underscore-dangle": "off"
    }
}
